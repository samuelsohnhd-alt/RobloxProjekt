local function nukeRB7UI(root)
    if not root then return end
    for _,d in ipairs(root:GetDescendants()) do
        if d:IsA("LocalScript") and d.Name == "RB7_UI" then
            d.Disabled = true
            d:Destroy()
            warn("[RB7_Preboot] entfernt: " .. d:GetFullName())
        end
    end
